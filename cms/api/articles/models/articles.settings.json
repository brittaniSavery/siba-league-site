{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "Article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Summary": {
      "type": "text",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "required": true
    },
    "Content": {
      "type": "richtext",
      "required": true
    },
    "Header": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "author": {
      "model": "authors"
    },
    "tags": {
      "collection": "tag"
    }
  }
}
