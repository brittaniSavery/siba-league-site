---
import { LEAGUE } from "@content/constants";

export type Props = {
  title: string;
  league: LEAGUE | string;
  file: string;
};

const { title, league, file } = Astro.props as Props;
---

<section class="hero is-fullheight">
  <iframe
    id="websitePage"
    {title}
    src={`${import.meta.env.BASE_URL}files/${league}/Website/${file}.html`}
    style="height: 100vh; width: 100%;"></iframe>
</section>
